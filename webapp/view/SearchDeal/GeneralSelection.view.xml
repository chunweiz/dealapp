							<f:FormElement label="{i18n>contractManagerAssigned}">
								<f:fields>
									<Input value=""/>
								</f:fields>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="{i18n>companyCode}">
								<f:fields>
									<Input value=""/>
								</f:fields>
							</f:FormElement>
								<f:FormElement label="{i18n>distributionChannel}">
								<f:fields>
									<Input value="" />
								</f:fields>
